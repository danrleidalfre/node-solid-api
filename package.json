{
  "name": "solid-api",
  "version": "1.0.0",
  "description": "Solid API",
  "main": "index.js",
  "scripts": {
    "development": "tsx watch src/server.ts",
    "production": "node build/server.js",
    "build": "tsup src --out-dir build",
    "lint": "eslint src --ext .ts --fix",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "test:create-env": "npm link ./prisma/vitest-environment-prisma",
    "test:install-env": "npm link vitest-environment-prisma",
    "pretest:e2e": "npm-run-all test:create-env test:install-env",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest --dir src/http",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rocketseat/eslint-config": "2.1.0",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "20.10.1",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "1.0.1",
    "@vitest/ui": "1.0.1",
    "all": "0.0.0",
    "eslint": "8.54.0",
    "npm": "10.2.5",
    "npm-run-all": "4.1.5",
    "prisma": "5.6.0",
    "run": "1.5.0",
    "supertest": "6.3.3",
    "tsup": "8.0.1",
    "tsx": "4.6.1",
    "typescript": "5.3.2",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "1.0.1"
  },
  "dependencies": {
    "@fastify/cookie": "9.2.0",
    "@fastify/jwt": "7.2.4",
    "@prisma/client": "5.6.0",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.10",
    "dotenv": "16.3.1",
    "fastify": "4.24.3",
    "zod": "3.22.4"
  }
}
